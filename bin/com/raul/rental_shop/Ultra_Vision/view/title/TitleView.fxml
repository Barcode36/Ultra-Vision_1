<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="titleAnchor" prefHeight="484.0" prefWidth="643.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.raul.rental_shop.Ultra_Vision.controller.title.TitleController">
   <children>
      <Pane fx:id="mainDiv" prefHeight="484.0" prefWidth="643.0">
         <children>
            <Label layoutX="311.0" layoutY="23.0" text="Search">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="searchBtn" layoutX="366.0" layoutY="18.0" prefHeight="34.0" prefWidth="254.0" promptText="Search..." />
            <TableView fx:id="table" layoutX="7.0" layoutY="77.0" prefHeight="334.0" prefWidth="629.0">
              <columns>
                <TableColumn fx:id="codeCol" prefWidth="110.0" text="Code" />
                <TableColumn fx:id="nameCol" prefWidth="119.0" text="Name" />
                  <TableColumn fx:id="costCol" prefWidth="107.0" text="Cost" />
                  <TableColumn fx:id="memberPlanCol" prefWidth="126.0" text="Member Plan" />
                  <TableColumn fx:id="yearCol" prefWidth="99.0" text="Year" />
                  <TableColumn fx:id="mediaCol" prefWidth="67.0" text="Media" />
              </columns>
            </TableView>
            <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="418.0" prefHeight="52.0" prefWidth="622.0" spacing="10.0">
               <children>
                  <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#actionDelete" text="Delete" />
                  <Button fx:id="updateBtn" mnemonicParsing="false" onAction="#actionEdit" text="Update" />
                  <Button fx:id="basketBtn" mnemonicParsing="false" onAction="#actionBasket" text="Basket" />
                  <Button fx:id="viewBtn" mnemonicParsing="false" onAction="#actionView" text="View" />
                  <Button fx:id="addBtn" mnemonicParsing="false" onAction="#actionAdd" text="Add" />
               </children>
            </HBox>
            <Label layoutX="30.0" layoutY="20.0" prefHeight="27.0" prefWidth="69.0" text="Titles">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
